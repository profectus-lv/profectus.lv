{
    "name": "eleventy-markdown-prime",
    "version": "5.4.1",
    "description": "A starter repository for a text-based blog site using the Eleventy site generator.",
    "type": "module",
    "license": "MIT",
    "author": {
        "name": "Dmitry Golubev",
        "url": "https://lastguru.lv"
    },
    "homepage": "https://emp-starter.lastguru.dev",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lastguru-net/emp.git"
    },
    "scripts": {
        "build": "npm run build:css && npm run build:js && npm run build:eleventy",
        "build:css": "postcss assets/css/site.css -o assets/css/site-build.css",
        "build:js": "esbuild assets/js/index.js --bundle --minify --outfile=assets/js/min.js --format=esm",
        "build:eleventy": "eleventy",
        "dev": "npm run build:css && npm run build:js && eleventy --serve"
    },
    "dependencies": {
        "@11ty/eleventy": "^3.1.2",
        "@11ty/eleventy-img": "^6.0.4",
        "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
        "eleventy-plugin-embed-everything": "^1.21.1",
        "xxhash-wasm": "^1.1.0",
        "json5": "^2.2.3",
        "esbuild": "^0.27.1",
        "cssnano": "^7.1.2",
        "luxon": "^3.7.2",
        "@minify-html/node": "^0.18.1",
        "htmlparser2": "^10.0.0",
        "markdown-it": "^14.1.0",
        "markdown-it-anchor": "^9.2.0",
        "markdown-it-deflist": "^3.0.0",
        "markdown-it-emoji": "^3.0.0",
        "markdown-it-footnote": "^4.0.0",
        "markdown-it-mark": "^4.0.0",
        "markdown-it-sub": "^2.0.0",
        "markdown-it-sup": "^2.0.0",
        "markdown-it-task-lists": "^2.1.1",
        "postcss": "^8.5.6",
        "postcss-cli": "^11.0.1",
        "tailwindcss": "^4.1.17",
        "@tailwindcss/postcss": "^4.1.17"
    }
}