{
    // Hide post reading time in post properties
    notime: [
        "events"
    ],
    // Hide these tags from the post properties
    notag: [
        "article",
        "posts",
        "events",
        "raksti",
        "authorcard"
    ],
    // Generate Event JSON-LD for these posts, instead of BlogPosting
    events: [
        "events"
    ],
    // Tag-specific icons (fallback: hashtag)
    icons: {
        pinned: "pin"
    }
}